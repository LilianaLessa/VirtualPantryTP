    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "yarn run eslint && yarn run tsc-test && yarn run jest",
    "eslint": "eslint .",
    "tsc-test": "tsc --project . --noEmit",
    "jest": "jest"
  },
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/elements": "^1.3.17",
